# =============================================================================
# 設定例ファイル - 実際の値に変更してterraform.tfvarsとして保存
# =============================================================================

project_name = "your-project-name"
environment  = "dev"
vpc_cidr     = "10.0.0.0/16"

# セキュリティ設定
allowed_ssh_cidr = "YOUR_IP_ADDRESS/32"  # 実際のIPに変更
key_name        = "your-key-name"        # 実際のキー名に変更

# データベース設定
db_username = "admin"
db_password = "CHANGE_TO_SECURE_PASSWORD"  # 強力なパスワードに変更

# API設定（以下のコマンドで生成してください）
# openssl rand -hex 32
api_key = "GENERATED_API_KEY_HERE"

# JWT設定（以下のコマンドで生成してください）
# openssl rand -base64 64
jwt_secret = "GENERATED_JWT_SECRET_HERE"

# アプリケーション設定
app_log_level = "INFO"
